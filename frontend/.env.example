# Frontend Configuration Example
# Vite loads environment variables with VITE_ prefix
# Copy to appropriate .env file for your environment

# API Configuration
VITE_API_URL=http://localhost:4000

# Environment
VITE_ENV=development
VITE_APP_NAME=DSA Learning Platform

# Debug (set to false in production)
VITE_DEBUG=true

# Analytics (optional)
VITE_ANALYTICS_ENABLED=false
VITE_ANALYTICS_ID=

# Error Tracking (optional - for Sentry)
VITE_SENTRY_DSN=

# Security
VITE_CSP_ENABLED=false
